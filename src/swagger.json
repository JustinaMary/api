            "type": "string"
          },
          {
            "name": "rewardPollDuration",
            "in": "body",
            "required": true,
            "type": "integer"
          },
          {
            "name": "proposeWithdrawPollDuration",
            "in": "body",
            "required": true,
            "type": "integer"
